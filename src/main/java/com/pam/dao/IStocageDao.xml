<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pam.dao.IStocageDao">
    <insert id="insertStocage">
        insert into stocage(quantite,Produits_idProduits)
        values(#{quantite}, #{Produits_idProduits})
    </insert>

    <select id="selectProduct" resultType="com.pam.beans.Product">
        select * from Product where Utilisateur_idUtilisateur = #{xxx}
    </select>
</mapper>